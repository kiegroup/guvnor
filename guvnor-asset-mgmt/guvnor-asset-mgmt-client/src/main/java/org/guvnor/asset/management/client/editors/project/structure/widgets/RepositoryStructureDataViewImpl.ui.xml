<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'>

    <ui:with field="i18n"
             type="org.guvnor.asset.management.client.i18n.Constants"/>

    <b:Form type="HORIZONTAL">
        <b:FieldSet>

            <b:Legend ui:field="projectTypeLabel"/>

            <b:FormGroup>
                <b:FormLabel ui:field="groupIdTextBoxHelpInline" addStyleNames="col-md-3"/>
                <b:Column size="MD_9">
                    <b:FormControlStatic ui:field="groupIdTextBox"/>
                </b:Column>
            </b:FormGroup>

            <b:FormGroup>
                <b:FormLabel ui:field="artifactIdTextBoxHelpInline" addStyleNames="col-md-3"/>
                <b:Column size="MD_9">
                    <b:FormControlStatic ui:field="artifactIdTextBox"/>
                </b:Column>
            </b:FormGroup>

            <b:FormGroup>
                <b:FormLabel ui:field="versionTextBoxHelpInline" addStyleNames="col-md-3"/>
                <b:Column size="MD_9">
                    <b:FormControlStatic ui:field="versionTextBox"/>
                </b:Column>
            </b:FormGroup>

        </b:FieldSet>
    </b:Form>

</ui:UiBinder>