<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'>

  <ui:with field="i18n"
           type="org.guvnor.asset.management.client.i18n.Constants"/>

    <b:FieldSet>

      <b:Heading ui:field="projectTypeLabel" size="H4"/>

      <b:HelpBlock ui:field="groupIdTextBoxHelpInline"/>
      <b:Label ui:field="groupIdTextBox"/>

      <b:HelpBlock ui:field="artifactIdTextBoxHelpInline"/>
      <b:Label ui:field="artifactIdTextBox"/>

      <b:HelpBlock ui:field="versionTextBoxHelpInline"/>
      <b:Label ui:field="versionTextBox"/>

    </b:FieldSet>

</ui:UiBinder>